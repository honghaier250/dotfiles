# Create zip file
zip archive.zip file1 directory/
zip -r archive.zip directory/

# Add file to zip
zip -m archive.zip filepath

# Update file to zip
zip -u archive.zip filepath

# Delete file in zip
zip -d archive.zip filepath

# To list, test and extract zip archives, see unzip
cheat unzip

zip -P koal111111 LiSi_Windows_5.3.7_2016_09_22_15_30_27.zip LiSi_Windows_5.3.7_2016_09_22_15_30_27.tar
